!!!Dzen

!dzen2.font: -*-fixed-*-*-*-*-*-*-*-*-*-*-*-*
dzen2.font: "xft: inconsolata-14" 
dzen2.foreground: #22EE11
dzen2.background: black

!!!urxvt

!URxvt*.backgroundPixmap: /home/quadrocube/urxvtbgs.png;style=tiled
URxvt*cursorBlink:		true
URxvt*scrollBar:		false
URxvt*.depth: 32
URxvt*.background: rgba:1111/1111/1111/dddd
URxvt*.font: xft:Consolas:pixelsize=13:style=bold
!urxvt*font: -artwiz-lime-medium-r-normal-*-10-*-*-*-*-*-*-*
!urxvt*boldFont: -artwiz-lime-medium-r-normal-*-10-*-*-*-*-*-*-*
!urxvt*italicFont: -artwiz-lime-medium-r-normal-*-10-*-*-*-*-*-*-*
!urxvt*bolditalicFont: -artwiz-lime-medium-r-normal-*-10-*-*-*-*-*-*-*
!URxvt.perl-ext-common:  default,tabbed

!!!Xft

Xft.dpi: 96
Xft.antialias: 1
Xft.hinting:    1
Xft.hintstyle:  hintfull
Xft.rgba:       rgb

!Digeratti
! Black
*color0:   #C0C0C0
*color8:   #003264

! Red
*color1:   #c03000
*color9:   #FF3A78

! Green
*color2:   #b1d631
*color10:  #A6CD07

! Yellow
*color3:   #FFCC66
*color11:  #B5C865

! Blue
*color4:   #0160C0
*color12:  #5E80BC

! Magenta
*color5:   #BC27BC
*color13:  #FF3B77

! Cyan
*color6:   #4bb5c1
*color14:  #4bb5c1

! White
*color7:   #e2e2e5
*color15:  #e2e2e5

*foreground: #C0C0C0

URxvt*cursorColor:  #FFFFBE

URxvt.colorIT:      #87af5f
URxvt.colorBD:      #d7d7d7
URxvt.colorUL:      #87afd7
URxvt.colorRV:      #87afd7

URxvt*troughColor:                    #87af5f
URxvt*highlightColor:                 #87af5f
URxvt*highlightTextColor:             #87af5f


!------------------------------------------------------------------!
!                  xoria256 terminal color scheme                  !
! http://snk.tuxfamily.org/log/xoria256-terminal-color-scheme.html !
!------------------------------------------------------------------!

!black.normal    : xoria256.LineNr.guibg
!black.bright    : black.normal.rgb - 8
!Rxvt.color0      : #121212
!Rxvt.color8      : #0a0a0a

!red.normal      : red.bright.r - 8
!red.bright      : xoria256.Special.guifg
!Rxvt.color1      : #d78787
!Rxvt.color9      : #df8787

!green.normal    : green.bright.g - 8
!green.bright    : xoria256.PreProc.guifg
!Rxvt.color2      : #afd787
!Rxvt.color10     : #afdf87

!yellow.normal   : yellow.bright.rg - 8
!yellow.bright   : xoria256.Special.guifg
!Rxvt.color3      : #f7f7af
!Rxvt.color11     : #ffffaf

!blue.normal     : blue.bright.b - 8
!blue.bright     : xoria256.Statement.guifg
!Rxvt.color4      : #87afd7
!Rxvt.color12     : #87afdf

!magenta.normal  : magenta.bright.rb - 8
!magenta.bright  : xoria256.Identifier.guifg
!Rxvt.color5      : #d7afd7
!Rxvt.color13     : #dfafdf

!cyan.normal     : cyan.bright.gb - 8
!cyan.bright     : green.bright + blue.bright
!Rxvt.color6      : #afd7d7
!Rxvt.color14     : #afdfdf

!white.normal    : white.bright.rgb - 8
!white.bright    : xoria256.Folded.guifg
!Rxvt.color7      : #e6e6e6
!Rxvt.color15     : #eeeeee

!foreground      : xoria256.Normal.guifg
!background      : xoria256.Normal.guibg
!cursor          : xoria256.Cursor.guibg
!Rxvt.foreground  : #d0d0d0
!Rxvt.background  : #1c1c1c
!Rxvt.cursorColor : #ffaf00
!Rxvt.cursorColor : #af5f5f 
!Rxvt.cursorColor : #eb1818

!bold            : xoria256.Type.guifg
!underline       : xoria256.htmlValue.guifg
!Rxvt.colorBD     : #afafdf
!Rxvt.colorUL     : #dfdfaf

URxvt*scrollTtyKeypress: true
URxvt*scrollTtyOutput: false
URxvt*saveLines: 120000

xscreensaver.newLoginCommand: lxdm -c USER_SWITCH
xscreensaver.fade: False
xscreensaver.mode: blank
xscreensaver.dpmsQuickOff: True
