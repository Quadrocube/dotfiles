# focus desktop
rcmd - 1 : chunkc tiling::desktop --focus 8
rcmd - 2 : chunkc tiling::desktop --focus 9
rcmd - 5 : chunkc tiling::desktop --focus 6
rcmd - 6 : chunkc tiling::desktop --focus 7
rcmd - 9 : chunkc tiling::desktop --focus 4

# move focused container to workspace
rcmd + shift - 1 : chunkc tiling::window --send-to-desktop 8
rcmd + shift - 2 : chunkc tiling::window --send-to-desktop 9
rcmd + shift - 5 : chunkc tiling::window --send-to-desktop 6
rcmd + shift - 6 : chunkc tiling::window --send-to-desktop 7
rcmd + shift - 9 : chunkc tiling::window --send-to-desktop 4
