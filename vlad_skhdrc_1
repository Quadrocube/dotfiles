# focus desktop
meh - 1 : chunkc tiling::desktop --focus 8
meh - 2 : chunkc tiling::desktop --focus 9
meh - 3 : chunkc tiling::desktop --focus 10
meh - 4 : chunkc tiling::desktop --focus 5
meh - 5 : chunkc tiling::desktop --focus 6
meh - 6 : chunkc tiling::desktop --focus 7
meh - 7 : chunkc tiling::desktop --focus 2
meh - 8 : chunkc tiling::desktop --focus 3
meh - 9 : chunkc tiling::desktop --focus 4

# move focused container to workspace
cmd + meh - 1 : chunkc tiling::window --send-to-desktop 8
cmd + meh - 2 : chunkc tiling::window --send-to-desktop 9
cmd + meh - 3 : chunkc tiling::window --send-to-desktop 10
cmd + meh - 4 : chunkc tiling::window --send-to-desktop 5
cmd + meh - 5 : chunkc tiling::window --send-to-desktop 6
cmd + meh - 6 : chunkc tiling::window --send-to-desktop 7
cmd + meh - 7 : chunkc tiling::window --send-to-desktop 2
cmd + meh - 8 : chunkc tiling::window --send-to-desktop 3
cmd + meh - 9 : chunkc tiling::window --send-to-desktop 4
